
<launch>
    <arg name="port_arg" default="/dev/ttyUSB0"/>
    <node pkg ="rosserial_python" type="serial_node.py" name="serial_node" output="screen" args="$(arg port_arg)">
	<param name="port" value="port_arg"/>
    </node>
    <node pkg ="gentle_ros" type="all_fingers_control.py" name="all_fingers_control"  output="screen">
    </node>
</launch>
